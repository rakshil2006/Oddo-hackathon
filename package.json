{
  "name": "skill-swap-platform",
  "version": "1.0.0",
  "description": "A modern web application for exchanging skills with others in the community",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "start": "cd server && npm start"
  },
  "keywords": [
    "skill-exchange",
    "community",
    "learning",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "Tech X",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
} 